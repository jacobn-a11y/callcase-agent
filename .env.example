# Required
OPENAI_API_KEY=<YOUR_OPENAI_API_KEY>

# Optional model override
OPENAI_MODEL=gpt-4o

# Provider configuration: merge or json
PROVIDER=merge

# Merge provider credentials (required if PROVIDER=merge)
MERGE_API_KEY=<YOUR_MERGE_API_KEY>
MERGE_ACCOUNT_TOKEN=<YOUR_MERGE_ACCOUNT_TOKEN>
MERGE_BASE_URL=https://api.merge.dev/api/filestorage/v1

# JSON provider input (required if PROVIDER=json)
JSON_INPUT_FILE=./sample-data/calls.json

# Output directory
OUTPUT_DIR=output
